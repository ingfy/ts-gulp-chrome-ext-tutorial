{
	"name": "ts-talk",
	"description": "StackOverflow code run buttons",
	"version": "0.0.1",
	"manifest_version": 2,
	"permissions": [],
	"web_accessible_resources": ["app/*.js"],
	"icons": {
		"128": "resources/icon128.png"
	},
	"content_scripts": [{
        "matches": [
            "*://*.stackoverflow.com/*"
        ],
        "js": ["contentScript.js"]
    }]
}